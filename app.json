{
  "expo": {
    "name": "OnlySeries",
    "slug": "OnlySeries",
    "version": "1.0.0",
    "scheme": "OnlySeries",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/onlyseries_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-web-browser",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/onlyseries_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/onlyseries_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dkvadim.myexpoapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/onlyseries_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dkvadim.myexpoapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "83fdbd5f-af06-46fa-b876-73e88c798559"
      }
    }
  }
}
